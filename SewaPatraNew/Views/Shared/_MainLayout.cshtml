                        </a>
                    </li>
                    <li>
                        <a style="text-decoration:none;" asp-area="" asp-controller="Report" asp-action="SewaPatraReceiptRegister">
                            <i class="bi bi-circle"></i><span>SewaPatra Receipt</span>
                        </a>
                    </li>
                    <li>
                        <a style="text-decoration:none;" asp-area="" asp-controller="Report" asp-action="SewaPatraDueDetails">
                            <i class="bi bi-circle"></i><span>SewaPatra Due Report</span>
                        </a>
                    </li>
                    <li>
                        <a style="text-decoration:none;" asp-area="" asp-controller="Report" asp-action="CollectionAnalysisReport">
                            <i class="bi bi-circle"></i><span>Collection Analysis</span>
                        </a>
                    </li>
                </ul>
            </li><!-- End Tables Nav -->
            @* <li class="nav-item">
                <a class="nav-link collapsed" asp-area="" asp-controller="Account" asp-action="Register">
                    <i class="bi bi-card-list"></i>
                    <span>Register</span>
                </a>
            </li> *@<!-- End Register Page Nav -->

           @*  <li class="nav-item">
                <a class="nav-link collapsed" asp-area="" asp-controller="Account" asp-action="Login">
                    <i class="bi bi-box-arrow-in-right"></i>
                    <span>Login</span>
                </a>
            </li> *@<!-- End Login Page Nav -->
        </ul>

    </aside><!-- End Sidebar-->

    <div id="main" class="main">
        @RenderBody()
    </div>

    <!-- ======= Footer ======= -->
    <footer id="footer" class="footer">
        <div class="copyright">
